<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.Pane?>

<?import java.net.URL?>
<VBox id="mainBox" fx:controller="Controller.startupWindow" fx:id="startupWindow" alignment="CENTER" maxHeight="-Infinity"
      maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
      xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Pane id="startImage" fx:id="starterImage" prefWidth="400" prefHeight="200">
            <VBox.margin>
                <Insets bottom="20.0"/>
            </VBox.margin>
        </Pane>
        <Button fx:id="startButton" mnemonicParsing="false" style="-fx-min-height: 50px; -fx-min-width: 75px;" text="Start"
                textAlignment="CENTER" onMouseClicked="#playerWindowMove">
            <font>
                <Font name="Calibri" size="20.0"/>
            </font>
        </Button>
        <Button fx:id="closeButton" mnemonicParsing="false" style="-fx-min-height: 50px; -fx-min-width: 75px;" text="Exit" onMouseClicked="#exitConfirm">
            <font>
                <Font name="Calibri" size="20.0"/>
            </font>
        </Button>
    </children>
    <stylesheets>
        <URL value="@style.css"/>
    </stylesheets>
</VBox>
